[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
windows.debug.x86_64 =   "res://rust/target/debug/rust_accelerator_.dll"
windows.release.x86_64 = "res://rust/target/release/rust_accelerator_.dll"
linux.debug.x86_64 =     "res://rust/target/debug/librust_accelerator_.so"
linux.release.x86_64 =   "res://rust/target/release/librust_accelerator_.so"
macos.debug =            "res://rust/target/debug/librust_accelerator_.dylib"
macos.release =          "res://rust/target/release/librust_accelerator_.dylib"
macos.debug.arm64 =      "res://rust/target/debug/librust_accelerator_.dylib"
macos.release.arm64 =    "res://rust/target/release/librust_accelerator_.dylib"
