[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
windows.debug.x86_64 =   "res://rust/target/debug/rust.dll"
windows.release.x86_64 = "res://rust/target/release/rust.dll"
linux.debug.x86_64 =     "res://rust/target/debug/rust.so"
linux.release.x86_64 =   "res://rust/target/release/rust.so"
macos.debug =            "res://rust/target/debug/rust.dylib"
macos.release =          "res://rust/target/release/rust.dylib"
